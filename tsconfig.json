{
	"compilerOptions": {
		"target": "esnext",
		"module": "esnext",
		"moduleResolution": "node",
		"baseUrl": "src",
		"jsx": "react",
		"noEmit": true,
		"isolatedModules": true,
		"allowSyntheticDefaultImports": true,
		"strict": true,
		"noFallthroughCasesInSwitch": true,
		"noImplicitReturns": true
	}
}