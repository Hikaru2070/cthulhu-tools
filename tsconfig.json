{
	"compilerOptions": {
		"target": "es5",
		"lib": [
			"esnext",
			"dom"
		],
		"module": "es2015",
		"moduleResolution": "node",
		"baseUrl": "src",
		"jsx": "react",
		"downlevelIteration": true,
		"allowSyntheticDefaultImports": true,
		"strict": true,
		"noFallthroughCasesInSwitch": true,
		"noImplicitReturns": true
	}
}