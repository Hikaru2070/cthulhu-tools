{
	"compilerOptions": {
		"target": "es2017",
		"module": "es2015",
		"moduleResolution": "node",
		"baseUrl": "src",
		"jsx": "react",
		"allowSyntheticDefaultImports": true,
		"strict": true,
		"noFallthroughCasesInSwitch": true,
		"noImplicitReturns": true
	},
	"exclude": [
		"node_modules",
		"old/**/*"
	]
}